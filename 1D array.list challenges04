#1d array/list challenges04

show1 = "vampire diaries"
show2 = "misfits"
show3 = "eastenders"
show4 = "harry potter"
print(show1)
print(show2)
print(show3)
print(show4)
show5=str(input("Enter a tv show >>> "))

tvshows = [show1, show2, show3, show4, show5]

location=int(input("where would you like your show to be placed in the list? 1,2,3,4 or 5? >>>"))

tvshows.insert(location,show5)

print(tvshows)
